{
  "name": "Parse Server Example",
  "description": "An example Parse API server using the parse-server module",
  "repository": "https://github.com/eggyo/my-parse-app.git",
  "logo": "https://avatars0.githubusercontent.com/u/1294580?v=3&s=200",
  "keywords": ["node", "express", "parse"],
  "env": {
    "PARSE_MOUNT": {
      "description": "Configure Parse API route.",
      "value": "/parse"
    },
    "APP_NAME": {
      "description": "Enter detail.",
      "value": "myAppName"
    },
    "APP_ID": {
      "description": "A unique identifier for your app.",
      "value": "myAppId"
    },
    "MASTER_KEY": {
      "description": "A key that overrides all permissions. Keep this secret.",
      "value": "myMasterKey"
    },
    "REST_KEY": {
      "description": "Enter detail REST_KEY",
      "value": "myRestKey"
    },
    "DASHBOARD_USER": {
      "description": "Enter DASHBOARD USERNAME",
      "value": "admin"
    },
    "DASHBOARD_PASS": {
      "description": "Enter DASHBOARD PASSWORD",
      "value": "pass"
    },
    "SERVER_URL": {
      "description": "URL to connect to your Heroku instance (update with your app's name + PARSE_MOUNT)",
      "value": "http://yourappname.herokuapp.com/parse"
    }
  },
  "image": "heroku/nodejs",
  "addons": ["mongolab"]
}
